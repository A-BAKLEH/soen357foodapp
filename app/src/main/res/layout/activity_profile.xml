<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E8F4EA"
    tools:ignore="textContrastCheck">

    <EditText
        android:id="@+id/text1"
        android:layout_width="272dp"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="131dp"
        android:layout_marginTop="225dp"
        android:drawablePadding="15dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textEmailAddress"
        android:padding="8dp"
        android:textColor='@color/blue'
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/txt2"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="129dp"
        android:layout_marginTop="278dp"
        android:layout_marginEnd="10dp"
        android:drawablePadding="15dp"
        android:ems="10"
        android:hint="Age"
        android:inputType="textEmailAddress"
        android:padding="8dp"
        android:textColor='@color/blue'
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/txt3"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="131dp"
        android:layout_marginTop="332dp"
        android:layout_marginEnd="8dp"
        android:drawablePadding="15dp"
        android:ems="10"
        android:hint="Weight"
        android:inputType="textEmailAddress"
        android:padding="8dp"
        android:textColor='@color/blue'
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/txt4"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="130dp"
        android:layout_marginTop="382dp"
        android:layout_marginEnd="9dp"
        android:drawablePadding="15dp"
        android:ems="10"
        android:hint="Diet Type"
        android:inputType="textEmailAddress"
        android:padding="8dp"
        android:textColor='@color/blue'
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/txt5"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="128dp"
        android:layout_marginTop="436dp"
        android:layout_marginEnd="11dp"
        android:drawablePadding="15dp"
        android:ems="10"
        android:hint="DCI"
        android:inputType="textEmailAddress"
        android:padding="8dp"
        android:textColor='@color/blue'
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="164dp"
        android:text="Profile"
        android:textColor="#233D4D"
        android:textSize="28sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="231dp"
        android:layout_height="22dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="156dp"
        android:text="Welcome to your profile page!"
        android:textColor="#233D4D"
        android:translationX="-20px"
        android:translationY="250px" />

    <ImageView
        android:id="@+id/img"
        android:layout_width="105dp"
        android:layout_height="90dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="31dp"
        android:translationX="700px" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="85dp"
        android:layout_height="33dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="140dp"
        android:layout_marginEnd="285dp"
        android:text="Name"
        android:textColor="#233D4D"
        android:textSize="18sp"
        android:translationY="225px" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="78dp"
        android:layout_height="34dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="289dp"
        android:layout_marginBottom="27dp"
        android:text="Age"
        android:textColor="#233D4D"
        android:textSize="18sp"
        android:translationY="665px" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="85dp"
        android:layout_height="33dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="286dp"
        android:layout_marginBottom="38dp"
        android:text="Weight"
        android:translationY="335dp"
        android:textColor="#233D4D"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="35dp"
        android:text="Diet Type"
        android:textColor="#233D4D"
        android:textSize="18sp"
        android:translationX="18px"
        android:translationY="385dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="85dp"
        android:layout_height="33dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="313dp"
        android:layout_marginBottom="180dp"
        android:text="DCI"
        android:textColor="#233D4D"
        android:textSize="18sp"
        android:translationY="435dp" />

    <Button
        android:id="@+id/update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="102dp"
        android:backgroundTint="#233D4D"
        android:text="Update"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#F44336"
        android:textColorHint="#9F1D1D"
        android:textColorLink="#3F51B5"
        android:translationX="160dp"
        android:translationY="500dp" />

    <ImageButton
        android:id="@+id/infoBtn1"
        android:layout_width="25dp"
        android:layout_height="29dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="361dp"
        android:layout_marginBottom="318dp"
        android:backgroundTint="#233D4D"
        app:srcCompat="@android:drawable/ic_menu_info_details"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/infoBtn2"
        android:layout_width="25dp"
        android:layout_height="29dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="362dp"
        android:layout_marginBottom="269dp"
        android:backgroundTint="#233D4D"
        app:srcCompat="@android:drawable/ic_menu_info_details"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

</RelativeLayout>